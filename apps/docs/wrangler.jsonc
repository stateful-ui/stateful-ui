{
  "$schema": "https://cdn.jsdelivr.net/npm/wrangler@4/config-schema.json",
  "name": "stateful-ui-docs",
  "main": ".open-next/worker.js",
  "compatibility_date": "2026-02-06",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "routes": [
    {
      "pattern": "stateful-ui.com",
      "custom_domain": true
    },
    {
      "pattern": "www.stateful-ui.com",
      "custom_domain": true
    }
  ],
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "stateful-ui-docs"
    }
  ],
}
